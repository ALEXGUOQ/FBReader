<?xml version="1.0" encoding="UTF-8"?>
<keymap>
	<binding key="Ctrl+L" action="library"/>
	<binding key="Ctrl+Shift+L" action="networkLibrary"/>
	<binding key="Ctrl+O" action="addBook"/>
	<binding key="Ctrl+F" action="search"/>
	<binding key="Ctrl+Shift+G" action="findPrevious"/>
	<binding key="Ctrl+G" action="findNext"/>
	<binding key="Ctrl+Shift+F" action="toggleFullscreen"/>
	<binding key="Ctrl++" action="increaseFont"/>
	<binding key="Ctrl+-" action="decreaseFont"/>
	<binding key="Right" action="lineForward"/>
	<binding key="Left" action="lineBackward"/>
	<binding key="Down" action="pageForward"/>
	<binding key="Up" action="pageBackward"/>
	<binding key="PgDown" action="pageForward"/>
	<binding key="PgUp" action="pageBackward"/>
	<binding key="Space" action="pageForward"/>
	<binding key="&lt;MouseScrollDown&gt;" action="mouseScrollForward"/>
	<binding key="&lt;MouseScrollUp&gt;" action="mouseScrollBackward"/>
	<binding key="Esc" action="cancel"/>
	<binding key="Ctrl+C" action="copyToClipboard"/>
	<binding key="Ctrl+Home" action="gotoHome"/>
	<binding key="Ctrl+Right" action="nextTOCSection"/>
	<binding key="Ctrl+Left" action="previousTOCSection"/>
	<binding key="Ctrl+Z" action="undo"/>
	<binding key="Ctrl+Shift+Z" action="redo"/>
<!--
	<binding key="&lt;O&gt;" action="preferences"/>
	<binding key="&lt;C&gt;" action="toc"/>
	<binding key="&lt;G&gt;" action="gotoPageNumber"/>
	<binding key="&lt;Z&gt;" action="debugNL"/>
-->
</keymap>
